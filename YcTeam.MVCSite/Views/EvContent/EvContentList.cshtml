@model IEnumerable<YcTeam.DTO.Master.EvContentDto>

<p>
    @Html.ActionLink("Create New", "CreateEvContent")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContentCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Content)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ContentCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Content)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "EvContentEdit", new { id=item.Id }) |
            @Html.ActionLink("Details", "EvContentDetails", new { id=item.Id }) |
            @Html.ActionLink("Delete", "EvContentDelete", new { id=item.Id })
        </td>
    </tr>
}

</table>
